{
  "presets": [
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "useBuiltIns": false
      }
    ]
  ],
  "plugins": ["transform-es2015-modules-commonjs", "@babel/plugin-transform-runtime", "dynamic-import-node"],
  "env": {
    "test": {
      "presets": ["@babel/preset-env", "@babel/preset-react"],
      "plugins": ["transform-es2015-modules-commonjs", "@babel/plugin-transform-runtime", "dynamic-import-node"]
    }
  }
}
